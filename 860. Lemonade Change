class Solution:
    def lemonadeChange(self, bills: List[int]) -> bool:
        cashbox = [0,0,0]
        for bill in bills:
            if bill == 5:
                cashbox[0] += 1
            if bill == 10:
                if cashbox[0] >= 1:
                    cashbox[1] += 1
                    cashbox[0] -= 1
                else:
                    return False
            if bill == 20:
                if cashbox[0] >= 3 or (cashbox[0] >= 1 and cashbox[1] >= 1):
                    if cashbox[0] >= 1 and cashbox[1] >= 1:
                        cashbox[0] -= 1
                        cashbox[1] -= 1
                    elif cashbox[0] >= 3:
                        cashbox[0] -= 3
                else: return False
        return True
            
